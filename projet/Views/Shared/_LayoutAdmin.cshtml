                        <a href="javascript:;">
                            <i class="fa fa-edit"></i>
                            <span>Filiere registration</span>
                        </a>
                        <ul class="sub">
                            <li> <a asp-area="" asp-controller="Admins" asp-action="Addfiliere">Add Filière</a></li>
