@model IEnumerable<projet.Models.Enseignant>

@{
    Layout = "_LayoutAdmin";
}



<section id="main-content">
    <section class="wrapper">
        <h3 class="bg-success"><i class="fa fa-list  "></i> @ViewBag.msg</h3>
        <div class="row">
            <div class="col-md-12">
                <div class="content-panel">
                    <h4><i class="fa fa-list  "></i> The list of added teachers</h4>
                    <hr>
                    <table class="table">
                        <thead>
                            <tr class="bg-success">

                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Username</th>
                                <th>Password</th>
                                <th>Email</th>
                                <th>Number Phone</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.nom)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.prenom)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.username)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.mdp)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.email)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.telephone)
                                    </td>


                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>
</section>
